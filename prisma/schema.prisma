// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

//criar uma migration: yarn prisma migrate dev

model Device {
  id           String  @id @default(uuid())
  name         String
  code         String
  cash_price   String
  term_price   String
  parcel       String
  value_parcel String
  location     Boolean

  create_at DateTime? @default(now())
  update_at DateTime? @default(now())

  @@map("device")
}

model Location {
  id           String @id @default(uuid())
  code         String
  region       String
  city         String
  store        String
  id_store     String
  cash_price   String
  term_price   String
  parcel       String
  value_parcel String

  create_at DateTime? @default(now())
  update_at DateTime? @default(now())

  @@map("location")
}
